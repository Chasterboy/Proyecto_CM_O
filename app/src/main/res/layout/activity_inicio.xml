<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondo"
    tools:context=".view.activities.inicio">

    <Button
        android:id="@+id/idPersonal"
        android:layout_width="202dp"
        android:layout_height="52dp"
        android:layout_marginBottom="152dp"
        android:onClick="EnviarPersonal"
        android:text="@string/boton_personal"
        android:fontFamily="@font/harry"
        app:cornerRadius ="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:backgroundTint="#656565"
        android:textSize="22dp"


        />

    <Button
        android:id="@+id/idEstudiantes"
        android:layout_width="207dp"
        android:layout_height="52dp"
        android:layout_marginBottom="32dp"
        android:text="@string/Boton_estudiantes"
        android:onClick="EnviarEstudiante"
        android:fontFamily="@font/harry"

        app:layout_constraintBottom_toTopOf="@+id/idPersonal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.514"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.975"
        app:cornerRadius ="20dp"
        android:backgroundTint="#656565"
        android:textSize="22dp"



        />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="227dp"
        android:layout_height="258dp"
        android:layout_marginTop="88dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/escudo" />


</androidx.constraintlayout.widget.ConstraintLayout>